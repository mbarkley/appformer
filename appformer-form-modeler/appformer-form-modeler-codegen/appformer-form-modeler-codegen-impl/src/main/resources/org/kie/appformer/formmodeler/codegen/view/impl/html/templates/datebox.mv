            <div id="@{field.name}_form_group" class="form-group">
                <label class="control-label" for="@{field.name}">@{field.label}</label>
                <div id="@{field.name}" placeholder="@{field.placeHolder}"
                    @if{field.readonly}
                       disabled="disabled"
                    @end{}>
                </div>
                <p id="@{field.name}_help_block" class="help-block"></p>
            </div>
